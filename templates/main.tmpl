[% title = "Harp Layout" %]
[% PROCESS 'inc/header.tmpl' %]



<body>
<div id="container">
<div id="header">
<h1>Harp Layout</h1>
</div> <!-- header -->

[% CGI.start_form('-method' => 'post') %]

<div id="leftcol">

    <div class="spacer">&nbsp;</div>
</div> <!-- leftcol -->

<div id="content">

<p>Welcome to Harp Layout.</p>


<p>Harp Key is [% harp.key %]. Position is [% harp.position %].  Position key is [% harp.position_key %].</p>

[% PROCESS 'inc/key_pos_options.tmpl' %]

<div class="harp_and_options">
    <div class="harp_table">
        [% PROCESS 'inc/harptable.tmpl' %]
    </div>
   
    <div class="harp_options">
        [% PROCESS 'inc/tuning_options.tmpl' %]
        [% PROCESS 'inc/view_options.tmpl' %]
    </div>
</div>

[% PROCESS 'form/hidden_fields.tmpl' %]
</form>


<p>Debug: [% debug %]</p>
</div> <!-- content -->

<div id="footer">
<p>Hello, I'm a footer</p>
</div> <!-- footer -->

</div> <!-- Container -->
</body>
</html>




