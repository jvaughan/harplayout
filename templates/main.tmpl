[% USE CGI ('-no_xhtml', 'true') %]

[% title = "Harp Layout" %]
[% PROCESS 'inc/header.tmpl' %]

<body>
<div id="container">
<div id="header">
<h1>Harp Layout</h1>
</div> <!-- header -->

<div id="leftcol">
    <div class="spacer">&nbsp;</div>
</div> <!-- leftcol -->

<div id="content">

[% CGI.start_form('-method' => 'post') %]

<div class="key_pos_container">
    [% PROCESS 'inc/key_pos_options.tmpl' %]
</div>

<div class="harp_and_options">
    <p>[% INCLUDE 'inc/nicepos.tmpl' pos = harp.position %] position harp in key of [% harp.key %] for song in [% harp.position_key %].  Harp tuning is [% harp.tuning %]</p>
    <div class="harp_table">
        [% PROCESS 'inc/harptable.tmpl' %]
    </div>
   
    <div class="harp_options">
        [% PROCESS 'inc/tuning_options.tmpl' %]
        [% PROCESS 'inc/view_options.tmpl' %]
    </div>
</div>

[% PROCESS 'form/hidden_fields.tmpl' %]
</form>


[% IF debug %]<p>Debug: [% debug %]</p>[% END %]
</div> <!-- content -->

<div id="footer">
    <p>&nbsp;</p>
</div> <!-- footer -->

</div> <!-- Container -->
</body>
</html>




